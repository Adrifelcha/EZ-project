	pages = {5472--5477},
	file = {Full Text PDF:/home/magikarp/snap/zotero-snap/common/Zotero/storage/XZBWPPE4/Enkavi et al. - 2019 - Large-scale analysis of test–retest reliabilities .pdf:application/pdf},
