	pmid = {30842284},
	note = {Publisher: National Academy of Sciences
Section: Social Sciences},
	keywords = {individual differences, self-regulation, retest reliability},
	pages = {5472--5477},
	file = {Full Text PDF:/home/magikarp/snap/zotero-snap/common/Zotero/storage/9P8F4L36/Enkavi et al. - 2019 - Large-scale analysis of test–retest reliabilities .pdf:application/pdf;Snapshot:/home/magikarp/snap/zotero-snap/common/Zotero/storage/GPVXNCV2/5472.html:text/html},
