	keywords = {Auditory Feedback, Change Type, Localist Account, Part Structure, Straight Edge},
	pages = {1253--1260},
	file = {Full Text PDF:/home/magikarp/snap/zotero-snap/common/Zotero/storage/2APRQIU5/Vandekerckhove et al. - 2007 - The concavity effect is a compound of local and gl.pdf:application/pdf},
