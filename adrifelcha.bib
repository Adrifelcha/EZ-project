	year = {2016},
	note = {Place: US
Publisher: American Psychological Association},
	keywords = {Attention, Cognition, Mind Wandering, Short Term Memory, Executive Function, Inhibition (Personality), Schizotypy},
	pages = {1017--1048},
	file = {Accepted Version:/home/magikarp/snap/zotero-snap/common/Zotero/storage/KGAB969H/Kane et al. - 2016 - Individual differences in the executive control of.pdf:application/pdf;Snapshot:/home/magikarp/snap/zotero-snap/common/Zotero/storage/96AGFT7W/2016-29680-001.html:text/html},
