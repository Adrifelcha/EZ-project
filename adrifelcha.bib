	year = {2005},
	note = {Num Pages: 197-215
Place: Washington, US
Publisher: American Psychological Association
(US)},
	keywords = {Reaction Time, Attention Deficit Disorder with Hyperactivity (principal), Cognitive Impairment, Drug Therapy (principal), Individual Differences (principal), Methylphenidate (principal), Response Inhibition (principal)},
	pages = {197--215},
	file = {Full Text PDF:/home/magikarp/snap/zotero-snap/common/Zotero/storage/WKWX22QA/Ridderinkhof et al. - Delta Plots in the Study of Individual Differences.pdf:application/pdf},
